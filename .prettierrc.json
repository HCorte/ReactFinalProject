{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "printWidth": 80,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "singleAttributePerLine": true,
    "overrides": [
        {
            "files": ["*.html", "*.css", "*.scss", "*.sass", "*.less"],
            "options": {
                "singleQuote": true
            }
        }
    ]
}
